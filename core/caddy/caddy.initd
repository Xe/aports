#!/sbin/openrc-run

# Sample init.d file for alpine linux.

name=caddy
command="/usr/bin/$name"
command_args="-agree -email $caddy_email $caddyfile"
command_background="yes"

start_stop_daemon_args="--user $sample_user:$sample_group"
pidfile="/run/$name.pid"

depend() {
  use logger dns
	need net
	after firewall
}
